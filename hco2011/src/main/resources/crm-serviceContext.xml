						http://www.springframework.org/schema/tx
						http://www.springframework.org/schema/tx/spring-tx-2.0.xsd">
	
	<!-- 配置事物的传播特性 -->
	<tx:advice id="txAdvice" transaction-manager="transactionManager">
		<tx:attributes>
			<!-- 配置存储事物  -->
			<tx:method name="stor*" propagation="REQUIRED"/>
			<!-- 配置删除事物 -->
			<tx:method name="del*" propagation="REQUIRED"/>
			<!-- 配置失效事物 -->
			<tx:method name="dis*" propagation="REQUIRED"/>
			<!-- 配置有效事物 -->
			<tx:method name="ena*" propagation="REQUIRED"/>
			<!-- 配置修改事物 -->
			<tx:method name="edit*" propagation="REQUIRED"/>
			<!-- 配置未读事物 -->
			<tx:method name="unread*" propagation="REQUIRED"/>
			<!-- 配置启动工作流事物  -->
			<tx:method name="start*" propagation="REQUIRED"/>
			<tx:method name="sav*" propagation="REQUIRED"/>
			<tx:method name="readAll*" propagation="REQUIRED"/>
			<tx:method name="unReadAll*" propagation="REQUIRED"/>
			<!-- 其他操作事物 -->
			<tx:method name="*" read-only="true"/>
		</tx:attributes>
	</tx:advice>
	
	<!-- 配置哪些类，那些方法参与事物 -->
	<aop:config>
		<aop:pointcut id="allManagerMethod" expression="execution(* com.yongjun.tdms.service..*.*(..))"/>
		<aop:advisor pointcut-ref="allManagerMethod" advice-ref="txAdvice"/>
	</aop:config>
	
