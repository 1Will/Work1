	<!--生日提醒定时器  每天运行一次 00:01执行--> 
	<bean id="timeCreateBirthPlan" class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">
		<property name="targetObject">
			<ref local="myRemindManager"/>
		</property>
		<property name="targetMethod">
			<value>storInfoByTime</value>
		</property>
	</bean>
	<bean id="timeCalculateBirthTrigger" class="org.springframework.scheduling.quartz.CronTriggerBean">
		<property name="jobDetail">
			<ref local="timeCreateBirthPlan"/>
		</property>
		<property name="cronExpression">
			<value>0 01 0 * * ?</value>
	  	</property>
	</bean>

