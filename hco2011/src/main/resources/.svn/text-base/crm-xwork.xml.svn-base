	<package name="pluto" extends="pluto">
		<action name="error" class="nullAction">
			<result name="success" type="freemarker">
				/pages/error.ftl
			</result>
			<interceptor-ref name="defaultStack" />
		</action>
		<action name="login" class="loginAction">
			<result name="success" type="freemarker">
				/pages/login.ftl
			</result>
			<result name="logout" type="freemarker">
				/pages/login.ftl
			</result>
			<result name="missing" type="redirect">
				/setup/setupLicense.html
			</result>
			<result name="user" type="redirect">
				/setup/setupSupervisor.html
			</result>
			<interceptor-ref name="localeStack" />
			<interceptor-ref name="setup" />
			<interceptor-ref name="propertySender">
				<param name="propertyNames">user</param>
			</interceptor-ref>
		</action>
