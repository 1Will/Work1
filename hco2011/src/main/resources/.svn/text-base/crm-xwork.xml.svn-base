	<package name="communication" namespace="/communication/groups"
		extends="pluto">
		<action name="upMoveUser" class="editCommunicationGroupAction"
			method="move">
		</action>
		<action name="downMoveUser" class="editCommunicationGroupAction"
			method="downMove">
		</action>
		<action name="editGroup" class="editCommunicationGroupAction">
			<result name="success" type="freemarker">
				/pages/base/communication/groups/groupInfo.ftl
			</result>
			<interceptor-ref name="propertyReceiverStack" />
		</action>
		<action name="saveGroup" class="editCommunicationGroupAction"
			method="save">
			<result name="new" type="redirect">
				editGroup.html?group.id=${group.id}
			</result>
			<result name="invalid.token" type="redirect">
				editGroup.html?group.id=${group.id}
			</result>
			<result name="success" type="redirect">
				editGroup.html?group.id=${group.id}
			</result>
			<result name="deleteSucess" type="redirect">
				listGroups.html
			</result>
			<result name="input" type="redirect">
				editGroup.html?group.id=${group.id}
			</result>
			<result name="error" type="redirect">editGroup.html</result>
			<interceptor-ref name="propertySender">
				<param name="propertyNames">group</param>
			</interceptor-ref>
			<interceptor-ref name="propertySenderStack" />
		</action>
		<action name="listGroups" class="listCommunicationGroupsAction"
			method="list">
			<result name="success" type="freemarker">
				/pages/base/communication/groups/groupList.ftl
			</result>
			<interceptor-ref name="parameterReceiverStack" />
		</action>
		<action name="searchGroups"
			class="listCommunicationGroupsAction">
			<result name="success" type="redirect">
				listGroups.html
			</result>
			<result name="invalid.token" type="redirect">
				listGroups.html
			</result>
			<interceptor-ref name="parameterSenderStack" />
		</action>
	</package>
	<package name="manual" namespace="/base/manual" extends="pluto">
		<action name="listManuals" class="listManualAction"
			method="list">
			<result name="success" type="freemarker">
				/pages/base/helpManual/manualList.ftl
			</result>
			<interceptor-ref name="parameterReceiverStack" />
		</action>
		<action name="searchManuals" class="listManualAction">
			<result name="success" type="redirect">
				listManuals.html
			</result>
			<result name="error" type="redirect">
				listManuals.html
			</result>
			<result name="invalid.token" type="redirect">
				listManuals.html
			</result>
			<interceptor-ref name="parameterSenderStack" />
		</action>
		<action name="editManual" class="editManualAction">
			<result name="success" type="freemarker">
				/pages/base/helpManual/manualProfile.ftl
			</result>
			<interceptor-ref name="propertyReceiverStack" />
		</action>
		<action name="saveManual" class="editManualAction"
			method="save">
			<result name="input" type="redirect">
				editManual.html?manual.id=${manual.id}
			</result>
			<result name="new" type="redirect">
				editManual.html?manual.id=${manual.id}
			</result>
			<result name="success" type="redirect">
				editManual.html?manual.id=${manual.id}
			</result>
			<result name="error" type="redirect">
				editManual.html?manual.id=${manual.id}
			</result>
			<interceptor-ref name="fileUploadStack" />
			<interceptor-ref name="propertySender">
				<param name="propertyNames">manual</param>
			</interceptor-ref>
			<interceptor-ref name="propertySenderStack" />
		</action>
		<action name="downloadManual" class="listManualAction"
			method="download">
			<result name="success" type="freemarker">
				/pages/base/helpManual/manualList.ftl
			</result>
			<result name="download" type="stream">
				<param name="inputName">inputStream</param>
				<param name="contentType">${contentType}</param>
				<param name="contentDisposition">
					attachment;filename="${fileName}"
				</param>
				<param name="bufferSize">2048</param>
			</result>
			<interceptor-ref name="parameterReceiverStack" />
		</action>
	</package>
