	<queryString><![CDATA[select device.device_no as [tooling.deviceNo],
        device.name as [tooling.name],
        device.graph_no as [tooling.graphNo],
        device.process_no as  [tooling.processNo],
        dept.name as [tooling.department.name],
        product.name as [productModel.name],
        washDetail.plan_wash_date as planWashDate,
        washDetail.proc_wash_date as procWashDate,
        dutypeople.name as [dutyPeople.name],
        supervisepeople.name as [supervisePeople.name],
        washDetail.wash_result as washResultTxt,
        washDetail.comment as comment
from t_wash wash left join t_department dept on wash.dept_id = dept.id, 
      (t_wash_detail washDetail left join t_device_card device on washDetail.tooling_id = device.id)
      left join t_users dutyPeople on washDetail.duty_people_id = dutypeople.id
      left join t_users supervisepeople on washDetail.supervise_people_id = supervisepeople.id
      left join t_product product on washDetail.product_model = product.id
where wash.id = washDetail.wash_plan_id]]></queryString>

