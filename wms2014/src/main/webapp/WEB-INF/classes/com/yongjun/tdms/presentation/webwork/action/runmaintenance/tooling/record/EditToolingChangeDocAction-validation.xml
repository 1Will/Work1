 <!-- $Id: EditToolingChangeDocAction-validation.xml 10840 2008-02-01 01:58:31Z qsun $-->       
<validators>
	<field name="toolingChangeDoc.name">
        <field-validator type="requiredstring">
            <message key="toolingChangeDoc.name.requiredstring"/>
        </field-validator>
        <field-validator type="stringlength">
            <param name="maxLength">50</param>
            <message key="toolingChangeDoc.name.maxLength"/>
        </field-validator>
	</field>	
    <field name="toolingChangeDoc.fileNo">
        <field-validator type="requiredstring">
            <message key="toolingChangeDoc.fileNo.requiredstring"/>
        </field-validator>
        <field-validator type="stringlength">
            <param name="maxLength">50</param>
            <message key="toolingChangeDoc.fileNo.maxLength"/>
        </field-validator>
	</field>
	<field name="toolingChangeDoc.description">
        <field-validator type="stringlength">
            <param name="maxLength">250</param>
            <message key="toolingChangeDoc.comment.maxLength"/>
        </field-validator>
	</field>
</validators>
