<validators>
    <field name="proc.name">
        <field-validator type="requiredstring">
            <message key="proc.name.requiredstring"/>
        </field-validator>
        <field-validator type="stringlength">
            <param name="minLength">0</param>
            <param name="maxLength">50</param>
            <message key="proc.name.maxLength"/>
        </field-validator>
	</field>
	<field name="proc.procExecTime">
        <field-validator type="date">
            <message key="plan.scheduleTime.date"/>
        </field-validator>
	</field>
	<field name="proc.summary">      
        <field-validator type="stringlength">
            <param name="minLength">0</param>
            <param name="maxLength">250</param>
            <message key="proc.request.maxLength"/>
        </field-validator>
	</field>
