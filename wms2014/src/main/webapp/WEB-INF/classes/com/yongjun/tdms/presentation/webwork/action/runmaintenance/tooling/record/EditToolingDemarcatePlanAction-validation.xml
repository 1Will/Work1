<validators>
	<field name="toolingDemarcatePlan.planName">
        <field-validator type="requiredstring">
            <message key="toolingDemarcatePlan.planName.requiredstring"/>
        </field-validator>
        <field-validator type="stringlength">
            <param name="maxLength">50</param>
            <message key="toolingDemarcatePlan.planName.maxLength"/>
        </field-validator>
	</field>
	<field name="toolingDemarcatePlan.comment">
        <field-validator type="stringlength">
            <param name="maxLength">250</param>
            <message key="toolingDemarcatePlan.comment.maxLength"/>
        </field-validator>
	</field>
</validators>
