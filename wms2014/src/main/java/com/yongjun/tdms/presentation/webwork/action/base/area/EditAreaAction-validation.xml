<!-- $Id: EditAreaAction-validation.xml 6847 2007-09-05 00:51:42Z qsun $ -->
<validators>
    <field name="area.code">
        <field-validator type="requiredstring">
            <message key="area.code.requiredstring"/>
        </field-validator>
        <field-validator type="stringlength">
            <param name="maxLength">20</param>
            <message key="area.code.maxLength"/>
        </field-validator>
    </field>
    <field name="area.name">
        <field-validator type="requiredstring">
            <message key="area.name.requiredstring"/>
        </field-validator>
        <field-validator type="stringlength">
            <param name="maxLength">20</param>
            <message key="area.name.maxLength"/>
        </field-validator>
	</field>
</validators>